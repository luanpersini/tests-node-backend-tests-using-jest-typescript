{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "Luan Persini",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rimraf dist & nest build",
    "start": "nest start",
    "start:dev": "npm run prebuild & nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",    
    "test": "jest --passWithNoTests --no-cache",
    "test:cov": "npm test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "npm test -- --watch --runInBand -c jest-unit-config.js",
    "test:int": "npm test -- --watch --runInBand -c jest-integration-config.js",
    "test:e2e": "npm run prebuild & npm test -- --watchAll --verbose --config ./tests/jest-e2e.json",
    "clear:jest": "jest --clearCache"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.6",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.6",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.6",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/testing": "^8.4.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.2.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.41",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "v16.15.1",
    "npm:": "v8.12.1"
  }
}
